<table>

  <thead>
    <tr>
      <th>Song Title</th>
      <th>Release year</th>
      <th>Artist Name</th>
      <th>Genre</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.release_year %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
      </tr>
    <% end %>
  </tbody>

</table>
